package Buses::SPI
public

  feature group SPI_Pins
  features
    MISO : in     event port; -- Master Input, Slave Output
    MOSI :    out event port; -- Master Output, Slave Input 
    SCLK : in out event port; -- Serial Clock
    SS   : in out event port; -- Slave select
  end SPI_Pins;

end Buses::SPI;
