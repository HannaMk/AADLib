package partitions
public

with POK;
with ARINC653;
with threads;
with SHM_DataType;


virtual processor acc_partition
properties
  --Scheduling_Protocol => (FixedTimeline);
  POK::Additional_Features => (console);
end acc_partition;

virtual processor implementation acc_partition.impl
end acc_partition.impl;
